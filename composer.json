{
    "name": "whitestork/bagisto-stripe-payment-gateway",
    "type": "package",
    "description": "The Laravel eCommerce Stripe Payment Gateway",
    "keywords": [
        "bagisto",
        "payment",
        "ecommerce",
        "stripe",
        "opensource"
    ],
    "license": "MIT",
    "homepage": "https://whitestorksoft.com",
    "minimum-stability": "dev",
    "support": {
        "source": "https://github.com/Whitestork-projects/Bagisto-Stripe-Payment-Gateway",
        "issues": "https://github.com/Whitestork-projects/Bagisto-Stripe-Payment-Gateway/issues"
    },
    "require": {
        "stripe/stripe-php": "^14.2"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\Stripe\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\Stripe\\Providers\\StripeServiceProvider"
            ],
            "aliases": {}
        }
    }
}
